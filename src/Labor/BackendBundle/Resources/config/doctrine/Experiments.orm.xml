<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Labor\BackendBundle\Entity\Experiments" table="experiments">
    <indexes>
      <index name="fk_expr_proj_idx" columns="project_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="false"/>
    <field name="description" type="string" column="description" length="450" nullable="true"/>
    <field name="createDate" type="date" column="create_date" nullable="false"/>
    <many-to-one field="project" target-entity="Projects">
      <join-columns>
        <join-column name="project_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
