<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Labor\BackendBundle\Entity\Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="profilImage" type="string" column="profil_image" length="160" nullable="false"/>
    <field name="createDate" type="date" column="create_date" nullable="false"/>
    <field name="password" type="string" column="password" length="444" nullable="false"/>
    <field name="type" type="integer" column="type" nullable="false"/>
    <field name="email" type="string" column="email" length="128" nullable="false"/>
    <field name="salt" type="blob" column="salt" length="16" nullable="false"/>
    <field name="lastname" type="string" column="lastname" length="16" nullable="false"/>
    <field name="firstname" type="string" column="firstname" length="16" nullable="false"/>
    <field name="username" type="string" column="username" length="16" nullable="false"/>
  </entity>
</doctrine-mapping>
